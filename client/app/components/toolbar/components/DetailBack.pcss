@import '../../../styles/variables.pcss';

.detail__back-button {
  color: var(--colour-white);
  display: flex;
  margin-left: auto;
  align-self: flex-end;
  background-color: var(--colour-black);
  position: fixed;
  right: 0;
  bottom: 0;
  cursor: pointer;
  width: 96px;
  height: 96px;

  i {
    width: 100%;
    height: 100%;
  }
}
